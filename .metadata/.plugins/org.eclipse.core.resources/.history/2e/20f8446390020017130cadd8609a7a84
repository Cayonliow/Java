package pig_latin;

public class PigLatin {

	@SuppressWarnings("null")
	public static void main(String[] args) {
		String[] data;
		data = new String[args.length];
		for (int i = 1; i < args.length; i++)
		{
			data[i] = new String(args[i]);
			System.out.println("cayon here= "+data[i]);
		}

	}
}
